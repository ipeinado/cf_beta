<% provide(:title, @project.title) %>
<div class="row">
	<div id="project-logo" class="col-md-3 col-md-offset-1">
		<%= image_tag @project.logo %>
	</div>
	<div class="col-md-6">
		<h1><%= @project.title %></h1>
		<p><%= @project.city %>, <%= @project.country %></p>
		<p><%= @project.description %></p>
		<%= image_tag @project.featured_picture %>
	</div>
</div>
